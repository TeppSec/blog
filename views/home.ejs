<%- include('partials/header'); -%>

    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>
    <% posts.forEach((post)=> { %>
        <div class="box">
            <h1>
                <%= post.title %>
            </h1>

            <p>
                <%= post.content.substring(0, 100) + ' ...' %>
                    <a href="/posts/<%= post._id %> ">ReadMore</a>
            </p>


        </div>


        <% } ) %>


            <%- include('partials/footer'); -%>